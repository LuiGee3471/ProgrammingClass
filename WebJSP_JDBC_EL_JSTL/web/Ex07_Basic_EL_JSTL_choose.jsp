<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%  request.setCharacterEncoding("UTF-8");  String name = request.getParameter("name");  if (name.equals("hong")) {      out.print("<b>" + name + "</b>");  } else if (name.equals("kim")) {      out.print("<i>" + name + "</i>");  } else {    out.print("<h3>" + name + "</h3>");  }  out.print("<hr>");%><html><head>  <title>Title</title></head><body><c:set var="name" value="<%=name%>" /><c:choose>  <c:when test="${param.name == 'hong'}">    <b>${name}</b>  </c:when>  <c:when test="${param.name == 'kim'}">    <i>${name}</i>  </c:when>  <c:otherwise>    <h3>${name}</h3>  </c:otherwise></c:choose></body></html>